{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/app/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/app/main.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1"
    }
}